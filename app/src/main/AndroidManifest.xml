<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sellerapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.Accountdetails"
            android:label="Account Details"
            android:icon="@color/white"
            android:roundIcon="@color/white"
            android:parentActivityName=".Activity.Addcompany"
            android:theme="@style/appthet"></activity>
        <activity
            android:name=".Activity.Addcompany"
            android:label="Add Company Details"
            android:icon="@color/white"
            android:roundIcon="@color/white"
            android:parentActivityName=".Activity.Password"
            android:theme="@style/appthet" />
        <activity android:name=".Activity.Password" />
        <activity android:name=".Activity.Login" />
        <activity android:name=".Activity.NewUserotp" />
        <activity android:name=".Activity.Newuser" />
        <activity
            android:name=".Activity.Splash"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.MainActivity" />
    </application>

</manifest>